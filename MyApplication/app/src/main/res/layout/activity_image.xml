<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/chairimage"
        android:src="@drawable/chairimage"
        android:layout_gravity="center"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="96dp" />

    <ImageView
        android:layout_width="151dp"
        android:layout_height="151dp"
        android:id="@+id/imageViewR1"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="151dp"
        android:layout_height="151dp"
        android:id="@+id/imageViewL1"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="151dp"
        android:layout_height="151dp"
        android:id="@+id/imageViewR2"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="151dp"
        android:layout_height="151dp"
        android:id="@+id/imageViewL2"
        android:layout_alignTop="@+id/imageViewR2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="151dp"
        android:layout_height="151dp"
        android:id="@+id/imageViewR3"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="151dp"
        android:layout_height="151dp"
        android:id="@+id/imageViewL3"
        android:layout_alignTop="@+id/imageViewR3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>